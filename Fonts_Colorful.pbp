; This patch allows using different colors for different characters

; hook into beginning of that same proc
82 b0 2d e9 f0 4f @ 99 b0 07 46 {
	; was:
	;SUB SP, SP, 0x64
	;MOV R7, R0

	B.W mycode2
	global cont2
}

{
	global mycode2
	; can use R5-R11
	; R1 is codepoint
	; R0 is ctx

	; let's just use codepoint last byte as color
	STRB R1, [R0, 0x2f]
;	BL log_r1
	
	SUB SP,SP,0x64
	MOV R7, R0

	B.W cont2
}
